<!--ユーザのログインフォーム画面-->

<%= form_with(scope: :sesstion, local: true,url: login_path) do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>


  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
